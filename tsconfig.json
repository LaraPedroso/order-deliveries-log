{
    "compilerOptions": {
        "target": "ES2022",
        "lib": ["ES2023"],
        "module": "node16",
        "paths": {
            "@/*": ["./src/*"]
        },
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "include": ["src", "knexfile.ts"]
}
